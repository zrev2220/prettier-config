{
  "name": "@zrev2220/prettier-config",
  "version": "1.0.0",
  "description": "My personal Prettier config",
  "main": ".prettierrc.json",
  "files": [
    ".prettierrc.json"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "run-s prettier-check",
    "lint-fix": "run-s prettier",
    "prettier": "prettier -u --ignore-path .gitignore --write .",
    "prettier-check": "prettier -u --ignore-path .gitignore --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrev2220/prettier-config.git"
  },
  "keywords": [
    "prettier"
  ],
  "author": "https://github.com/zrev2220",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zrev2220/prettier-config/issues"
  },
  "homepage": "https://github.com/zrev2220/prettier-config#readme",
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3"
  },
  "lint-staged": {
    "*": "prettier -u --ignore-path .gitignore --write"
  }
}
